<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dongzhe.mycarpool"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/tubiao3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.dongzhe.mycarpool.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            =
        </activity>
        <activity android:name="com.example.dongzhe.mycarpool.MainActivity" >
            <activity android:name="com.example.dongzhe.mycarpool.Register" />
            <activity android:name="com.example.dongzhe.mycarpool.NeedHelp" />
            <activity android:name="com.example.dongzhe.mycarpool.NewPassword" />
            <activity android:name="com.example.dongzhe.mycarpool.DriverMain" />
            <activity android:name="com.example.dongzhe.mycarpool.PassengerBrowse" />
            <activity android:name="com.example.dongzhe.mycarpool.PassengerImformation" />
            <activity android:name="com.example.dongzhe.mycarpool.PassengerMain" />
            <activity android:name="com.example.dongzhe.mycarpool.Client" />
        </activity>
        <activity android:name="com.example.dongzhe.mycarpool.SplashtwoActivity" />
    </application>

</manifest>