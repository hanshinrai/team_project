<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rbnwxj.unblocking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DriverImformationActivity" />
        <activity android:name=".DriverRaiseActivity" />
        <activity android:name=".driver_browse" />
        <activity android:name=".PassengerImformation" />
        <activity android:name=".PassengerBrowse"></activity>
    </application>

</manifest>